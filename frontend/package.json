  "version": "1.0.0",
  "description": "Frontend application for Teddy Anangwe's Portfolio Website",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
